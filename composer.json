{
    "name": "jskowronski39/programa-test",
    "description": "",
    "license": "proprietary",
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "autoload": {
        "psr-4": {
            "ProgramaTest\\": "src/"
        }
    },
    "scripts": {
        "cs": [
            "php vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix"
        ],
        "tests": [
            "php vendor/phpunit/phpunit/phpunit --bootstrap vendor/autoload.php tests --testdox"
        ],
        "orm-cli": [
            "php vendor/doctrine/orm/bin/doctrine"
        ]
    },
    "require": {
        "doctrine/orm": "^2.6",
        "twig/twig": "^2.6"
    }
}
